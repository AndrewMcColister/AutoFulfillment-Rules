<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <mirrors>
        <mirror>
            <id>delta-maven-nexus</id>
            <n>Delta Enterprise AWS Nexus</n>
            <url>https://nexus.enterprise-tools.aws.delta.com/repository/maven-public/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>snapshots</id>
            <username>${env.MVN_USERNAME}</username>
            <password>${env.MVN_PASSWORD}</password>
        </server>
        <server>
            <id>releases</id>
            <username>${env.MVN_USERNAME}</username>
            <password>${env.MVN_PASSWORD}</password>
        </server>
    </servers>
</settings>
